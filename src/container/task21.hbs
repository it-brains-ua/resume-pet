<main class="p-5">
    {{#with heading}}
    <section>
        <div class="card">
            <div class="card-body">
                {{#with main}}
                <div class="card">
                    <div class="card-body">
                        <h1>{{title}}</h1>
                        <p>{{text}}</p>
                        {{#with button_text}}
                        <button class="btn btn-primary">{{solid}}</button>
                        <br><br><br>
                        <button class="btn btn-outline-primary">{{outline}}</button>
                        {{/with}}
                    </div>
                </div>
                {{/with}}            
                
                <br>

                <div class="card">
                    {{#with second}}
                    <div class="card-body">
                        <h2>{{title}}</h2>
                        <p>{{text}}</p>
                        {{#with button_text}}
                        <button class="btn btn-secondary">{{solid}}</button>
                        <br><br>
                        <button class="btn btn-outline-secondary">{{outline}}</button>
                        {{/with}}
                    </div>
                    {{/with}}
                </div>
                
                <br>

                <div class="card">
                    {{#with subblock}}
                    <div class="card-body">
                        <h3>{{title}}</h3>
                        <p>{{text}}</p>
                        {{#with button_text}}
                        <button class="btn btn-dark">{{solid}}</button>
                        <br><br>
                        <button class="btn btn-outline-dark">{{outline}}</button>
                        {{/with}}
                    </div>
                    {{/with}}
                </div>
            </div>  
        </div>
    </section>
    {{/with}}

    <br>

    {{#with nested_display}}
    <section>
        <div class="card">
            <div class="card-body">
                   <div class="display-1">{{title}}</div>
                    <br>
                    {{#with children_display}}
                        <div class="card">
                            <div class="card-body">
                                <div class="display-4">{{title}}</div>
                                    <p>{{text}}</p>
                                    {{#with button}}
                                    <button class="btn btn-outline-warning">{{text}}</button>
                                    {{/with }}
                            </div>
                        </div>                
                    {{/with}}  
                    <br>
                    <h6>{{header_text}}</h6>
                    {{#with button}}
                        <button class="btn btn-info">{{text}}</button>
                    {{/with}}
                 
            </div>
        </div> 
    </section> 
    {{/with}}

    <br>

    {{#with paragraph}}
    <section>
        <div class="card">
            <div class="card-body">
                <p class="text-uppercase">{{title}}</p>
                <p class="text-lowercase">{{text}}</p>
                {{#with button}}
                {{>task21}}
                {{/with}}
                <br><br>
                
                <div class="card">
                    <div class="card-body">
                        {{#each list_name}}
                        <ul>
                            <li>
                                {{>task21}}
                            </li>
                        </ul>            
                        {{/each}}
                    </div>
                </div>
                
                
            </div>
        </div>

        
    </section>
    {{/with}}

    
</main>